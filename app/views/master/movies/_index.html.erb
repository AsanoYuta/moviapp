<table class='table table-hover table-inverse'>
  <thead>
    <tr>
      <th>投稿画像</th>
      <th>タイトル</th>

      <th colspan="2"></th>
    </tr>
  </thead>
  <tbody>
    <% movies.each do |movie| %>
      <tr>
        <td><%= link_to master_movie_path(movie.user.id) do %>
          <%= image_tag movie.get_image(50,50) %>
          <% end %>
        </td>
        <td><%= movie.title %></td>
      </tr>
    <% end %>
  </tbody>
</table>