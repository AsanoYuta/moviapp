 <%= form_with url: master_search_path,method: :get, local: true do |f| %>
   <%= f.text_field :content, size: "40x7" %>
   <%= f.select :model,options_for_select({"ユーザー"=>"user","映画"=>"movie"})%>
   <%= f.select :method,options_for_select({"完全一致"=>"perfect","前方一致"=>"forward","後方一致"=>"backward","部分一致"=>"partial"})%>
   <%= f.submit'検索', class: "btn btn-primary" %>
 <% end %>
