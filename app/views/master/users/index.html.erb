<div class="col-md-6 offset-md-1">
<h2>ユーザー一覧</h2>
<table class = "table">

<thead>
  <tr>
    <th>プロフィール画像</th>
    <th>ユーザー名</th>
    <th></th>
    <th>ステータス</th>
    <th colspan="3"></th>
  </tr>
</thead>

<tbody>
<% @users.each do |user| %>
 <tr>
  <td><%= image_tag user.get_profile_image(50,50) %>
  </td>
  <td><%= user.name %></td>
  <td><%= link_to "ユーザー詳細", master_user_path(user.id) %></td>
  <td><%= user.is_deleted_i18n %></td>
 </tr>
<% end %>
</tbody>
</table>


</div>
</div>
</div>